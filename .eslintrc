{
  "env": {
    "browser": true,
    //ecmascript6 ese nombre verdadero
    "es6":true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": "warn",
    "import/extensions": 0,
    //para reconoce set ..  funciona set
    "no-unused-vars": "off"
  }
}
